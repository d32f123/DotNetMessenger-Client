<Window x:Class="DotNetMessenger.WPFClient.Router.WPFViewModelPresenter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:DotNetMessenger.WPFClient.ViewModels"
        mc:Ignorable="d"
        Title="WPFViewModelPresenter" Height="Auto" Width="Auto" 
        d:DataContext="{d:DesignInstance viewModels:ViewModelBase}"
        SizeToContent="WidthAndHeight" ResizeMode="NoResize">
    <ContentPresenter Content="{Binding}"/>
</Window>
